{
	"name": "cno-plugin-responsive-blocks",
	"version": "1.0.1",
	"description": "A plugin to add responsive controls to blocks",
	"license": "GPL-3.0-or-later",
	"author": "Choctaw Nation of Oklahoma",
	"prettier": "@wordpress/prettier-config",
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"files":[
		"inc/",
		"cno-plugin-responsive-blocks.php",
		"build/",
		"readme.md"
	],
	"scripts": {
		"build": "wp-scripts build --blocks-manifest  --webpack-copy-php",
		"format": "wp-scripts format",
		"lint:css": "npx stylelint '**/*.scss' '**/*.css' --fix",
		"lint:js": "npx eslint --fix",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts build --blocks-manifest && wp-scripts plugin-zip",
		"start": "wp-scripts start --blocks-manifest  --webpack-copy-php"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/eslintrc": "^3.3.1",
		"@wordpress/block-editor": "^15.3.0",
		"@wordpress/blocks": "^15.3.0",
		"@wordpress/components": "^30.3.0",
		"@wordpress/data": "^10.30.0",
		"@wordpress/hooks": "^4.30.0",
		"@wordpress/scripts": "^30.23.0",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-react-hooks": "^5.2.0",
		"globals": "^16.4.0",
		"react": "^18.3.1",
		"stylelint": "^16.24.0",
		"stylelint-config-standard-scss": "^15.0.1",
		"stylelint-scss": "^6.12.1",
		"webpack-remove-empty-scripts": "^1.1.1"
	}
}
